<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="blockchain1.IssueCert" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Issue Certificate" showNavButton="true" navButtonPress="BackButtonPress" >
		<content>
	<GenericTile id="Tile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Single Certificate"
		 press="pressSingle">
		<tileContent>
			<TileContent>
				<content>
					<ImageContent src="sap-icon://add-activity-2"></ImageContent>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	
	<GenericTile id="Tile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Multiple Certificate" press="pressMulti">
		<tileContent>
			<TileContent>
				<content>
					<ImageContent src="sap-icon://add-activity"></ImageContent>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
		</content>
	</Page>
</core:View>